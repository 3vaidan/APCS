public class Pet {
  //////////// fields
  private String kind; //ex bird dog
  private String breed; //ex terrier
  private String name;
  private int age;
  
  //////////// constructors
  public Pet(String k) {
   kind = k;
   breed = null;
   name = null;
   age = 0;
  }
  
  public Pet(String k, String b) {
   kind = k;
   breed = b;
   name = null;
   age = 0;
  }
  
  public Pet(String k, String b, String n) {
   kind = k;
   breed = b;
   name = n;
   age = 0;
  }
  
  public Pet(String k, String b, String n, int a) {
   kind = k;
   breed = b;
   name = n;
   age = a;
  }
  //////////// methods
  //accesor
  public String getKind() {
   return kind; 
  }
  
  public String getBreed() {
   return breed; 
  }
  
  public String getName() {
   return name; 
  }
  
  public int getAge () {
    return age;
  }
  
  //mutator
  public void changeKind(String k) {
   kind = k; 
  }
  
  public void changeBreed(String b) {
   breed = b; 
  }
  
  public void changeName(String n) {
   name = n;
  }
  
  public void changeAge (int a) {
   age = a; 
  }
  
  public static void main(String[] args) {
    Pet pet = new Pet("dog","cockapoo","Matt",12);
    pet.changeAge(4);
    
    System.out.println(pet.getName());
  }
  
}