public class Song {
 
  private String title;
  private String artist;
  private int length;
  
  /** 
   * Construct class with parameters
   * @param t = String title
   * @param a = String artist
   * @param l = int length
   * @returns void
   */
  public void Song(String t,String a,int l) {
    title = t;
    artist = a;
    length = l;
  }
  
  /**Accessors */
  
  /**
   * Get the title of the song
   * @returns String title
   */
  public String getTitle() {
   return title; 
  }
  
  /**
   * Get the artist of the song
   * @returns String artist
   */
  public String getArtist() {
   return artist; 
  }
  
  /**
   * Get the length of the song
   * @returns int length
   */
  public int getLength() {
   return length; 
  }
  
  /** Mutators */
  
  /** 
    * Change the title of the song
    * @param t = String title
    * @returns void
    */
  public void changeTitle(String t) {
    title = t;
  }
  
  /** 
    * Change the artist of the song
    * @param a = String artist
    * @returns void
    */
  public void changeArtist(String a) {
    artist = a;
  }
  
  /** 
    * Change the length of the song
    * @param l = int length
    * @returns void
    */
  public void changeLength(int l) {
    length = l;
  }
  
  /**
   * Returns all attributes
   * @returns String
   */
  public String toString() {
   return title + " by " + artist + ". " + length + " seconds long."; 
  }
}