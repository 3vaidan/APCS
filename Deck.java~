
public class Deck {
  String[] suites = String["Spades","Hearts","Diamonds","Clubs"];
  String[] numbers = String["Two","Three","Four","Five","Six","Seven","Eight","Nine","Ten","Jack","Queen","King","Ace"];
  String[] cards = String[52];
  public void Deck() {
    this.makeDeck();
  }
  
  public void makeDeck() {
    counter = 0;
    for (int i = 0; i < numbers.length;i++) {
      for(int k = 0; k < suites.length;k++) {
       cards[counter] = numbers[i]+" of " + suites[k];
       counter++;
      }
    }
  }
  
  public String[] getDeck() {
   return cards; 
  }
  
}